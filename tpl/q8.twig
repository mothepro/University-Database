{% extends 'body.twig' %}

{% block title %}Query Six{% endblock %}

{% block body %}
	<h2>Students on Waitlist</h2>
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Banner Number</th>
				<th>Adviser ID</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Street</th>
				<th>City</th>
				<th>Post Code</th>
				<th>Phone Number</th>
				<th>Email</th>
				<th>Date of Birth</th>
				<th>Gender</th>
				<th>Nationality</th>
				<th>Special Needs</th>
				<th>Comments</th>
				<th>Status</th>
				<th>Major</th>
				<th>Minor</th>
				<th>Year</th>
			</tr>
		</thead>
		<tbody>
		{% for student in data %}
			<tr>
				<td>{{ student.bannerNumber }}</td>
				<td>{{ student.adviserID }}</td>
				<td>{{ student.firstName }}</td>
				<td>{{ student.lastName }}</td>
				<td>{{ student.street }}</td>
				<td>{{ student.city }}</td>
				<td>{{ student.postCode }}</td>
				<td>{{ student.phoneNO }}</td>
				<td>{{ student.email }}</td>
				<td>{{ student.dob  | date("F j\\<\\s\\u\\p\\>S\\<\\/\\s\\u\\p\\>")  | raw }}</td>
				<td>{{ student.gender }}</td>
				<td>{{ student.nationality }}</td>
				<td>{{ student.specialNeeds }}</td>
				<td>{{ student.comments }}</td>
				<td>{{ student.status }}</td>
				<td>{{ student.major }}</td>
				<td>{{ student.minor }}</td>
				<td>{{ student.year }}</td>
			</tr>
		{% endfor %}
		</tbody>
	</table>
{% endblock %}
	